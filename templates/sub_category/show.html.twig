{% extends 'base.html.twig' %}

{% block title %}{{ 'SubCategory'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'SubCategory'|trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sub_category.id }}</td>
            </tr>
            <tr>
                <th>{{ 'Name'|trans }}</th>
                <td>{{ sub_category.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sub_category_index') }}">{{ 'back to list'|trans }}</a>

    <a href="{{ path('app_sub_category_edit', {'id': sub_category.id}) }}">{{ 'edit'|trans }}</a>

    {{ include('sub_category/_delete_form.html.twig') }}
{% endblock %}
