{% if pageCount > 1 %}
<nav aria-label="Page navigation example">
  <ul class="pagination">
    {% if previous is defined %}
    <li class="page-item">
      <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): previous})) }}">{{ 'Previous'|trans }}</a>
    </li>
    {% endif %}
    
    {% for page in pagesInRange %}
        {% if page != current %}
        <li class="page-item">
          <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): page})) }}">{{ page }}</a>
        </li>
        {% else %}
        <li class="page-item active" aria-current="page">
          <a class="page-link" href="#">{{ page }}</a>
        </li>
        {% endif %}
    {% endfor %}
    
    {% if next is defined %}
    <li class="page-item">
      <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): next})) }}">{{ 'Next'|trans }}</a>
    </li>
    {% endif %}
  </ul>
</nav>
{% endif %}
