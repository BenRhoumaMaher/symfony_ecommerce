{% extends 'base.html.twig' %}

{% block body %}
	<div class="container">
		<table class="table">
			<tr>
				<th>id</th>
				<th>quantiy</th>
				<th>date added</th>
			</tr>
			{% for productAdded in addProductHistory %}
				<tr>
					<td>{{ productAdded.id }}</td>
					<td>{{ productAdded.qte }}</td>
					<td>{{ productAdded.createdAt|date('d-m-Y-H-i-s') }}</td>
				</tr>
			{% endfor %}
		</table>
	</div>
{% endblock %}
